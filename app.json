{
  "expo": {
    "name": "My Super Diaries",
    "slug": "diaries-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.mysuperdiaries"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization"
    ],
    "githubUrl": "https://github.com/ipr0310/my-super-diaries",
    "scheme": "my-super-diaries",
    "description": "Keep your secrets and diaries on your device locally!",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c19c4d63-ec76-41d8-b2c7-85724e33ae1f"
      }
    },
    "owner": "ipr0310"
  }
}
